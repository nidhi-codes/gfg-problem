// driver

#include <bits/stdc++.h>
using namespace std;

/* Linked list Node */
struct Node {
    int data;
    struct Node* next;
    Node(int x) {
        data = x;
        next = NULL;
    }
};

struct Node* buildList(int size)
{
    int val;
    cin>> val;
    
    Node* head = new Node(val);
    Node* tail = head;
    
    for(int i=0; i<size-1; i++)
    {
        cin>> val;
        tail->next = new Node(val);
        tail = tail->next;
    }
    
    return head;
}

void printList(Node* n)
{
    while(n)
    {
        cout<< n->data << " ";
        n = n->next;
    }
    cout<< endl;
}


// } Driver Code Ends
/* node for linked list:

struct Node {
    int data;
    struct Node* next;
    Node(int x) {
        data = x;
        next = NULL;
    }
};

*/

class Solution
{
    public:
    
    Node* reverse(Node* head){
        
        Node* prev = NULL;
        Node* curr = head;
        Node* forward = NULL;
        
        
        while(curr){
            
            forward = curr->next;
            curr->next = prev;
            prev = curr;
            curr = forward;
        }
        
        return prev;
    }
    //Function to add two numbers represented by linked list.
    struct Node* addTwoLists(struct Node* first, struct Node* second)
    {
        // code here
        
        first = reverse(first);
        second = reverse(second);
        
        Node* head = new Node(0);
        Node* temp = head;
        int carry = 0;
        int sum = 0;
        
        while(first && second){
            
            sum = first->data + second->data + carry;
            temp->next = new Node(sum%10);
            temp = temp->next;
            carry = sum/10;
            
            first = first->next;
            second = second->next;
        }
        
        while(first){
            sum = sum = first->data + carry;
            temp->next = new Node(sum%10);
            temp = temp->next;
            
            carry = sum/10;
            
            first = first->next;
        }
        
        while(second){
            sum = sum = second->data + carry;
             temp->next = new Node(sum%10);
            temp = temp->next;
            
            carry = sum/10;
            
            second = second->next;
        }
        
        if(carry > 0){
            temp->next = new Node(carry);
            temp = temp->next;
        }
        
        head = reverse(head->next);
        
        return head;
    }
};


//{ Driver Code Starts.

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n, m;
        
        cin>>n;
        Node* first = buildList(n);
        
        cin>>m;
        Node* second = buildList(m);
        Solution ob;
        Node* res = ob.addTwoLists(first,second);
        printList(res);
    }
    return 0;
}

// } Driver Code Ends
